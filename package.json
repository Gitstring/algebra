{
  "name": "algebra",
  "homepage": "http://g14n.info/algebra",
  "author": {
    "name": "fibo",
    "url": "http://g14n.info"
  },
  "version": "0.5.0",
  "description": "Vectors, Matrices, Tensors",
  "main": "index.js",
  "scripts": {
    "homepage": "echo \"---\ntitle: $npm_package_name\n---\" > gh-pages/index.md; cat README.md >> gh-pages/index.md",
    "package.json": "cp package.json gh-pages/_data",
    "gh-pages:_data": "npm run package.json; npm run dox",
    "gh-pages:push": "git subtree --prefix gh-pages push origin gh-pages",
    "gh-pages:pull": "git subtree --prefix gh-pages pull origin gh-pages",
    "jekyll": "npm run homepage; npm run gh-pages:_data; cd gh-pages; jekyll build; cd ..",
    "test": "mocha --bail --require should --reporter min --recursive",
    "browserify": "browserify -s $npm_package_name -e index.js -o dist/${npm_package_name}.js",
    "build": "npm test && npm run browserify",
    "jshint": "jshint -c .jshintrc src/*.js",
    "dox": "cat src/*.js | dox > gh-pages/_data/dox.json"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/fibo/algebra.git"
  },
  "keywords": [
    "algebra",
    "matrix",
    "vector",
    "tensor",
    "math"
  ],
  "license": [
    {
      "type": "MIT",
      "url": "http://g14n.info/mit-license.html"
    }
  ],
  "devDependencies": {
    "browserify": "^10.1.3",
    "dox": "^0.7.1",
    "mocha": "^2.2.4",
    "should": "^6.0.1"
  },
  "engines": {
    "node": ">=0.8"
  },
  "readmeFilename": "README.md",
  "dependencies": {
    "inherits": "~2.0.1",
    "strict-mode": "~0.1.4"
  }
}

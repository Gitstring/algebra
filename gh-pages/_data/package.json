{
  "name": "algebra",
  "homepage": "http://g14n.info/algebra",
  "author": {
    "name": "fibo",
    "url": "http://g14n.info"
  },
  "version": "0.4.0",
  "description": "Vectors, Matrices, Tensors",
  "main": "index.js",
  "scripts": {
    "homepage": "echo -e \"---\ntitle: $npm_package_name\n---\" > gh-pages/index.md; cat README.md >> gh-pages/index.md",
    "package.json": "cp package.json gh-pages/_data",
    "gh-pages": "npm run jekyll && git commit -a && git subtree --prefix gh-pages push origin gh-pages",
    "jekyll": "npm run homepage; npm run package.json; cd gh-pages; jekyll build; cd ..",
    "test": "mocha --bail --require should --reporter min --recursive",
    "browserify": "browserify -s $npm_package_name -e index.js -o dist/${npm_package_name}.js",
    "build": "npm test && npm run browserify",
    "jshint": "jshint -c .jshintrc src/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/fibo/algebra.git"
  },
  "keywords": [
    "algebra",
    "matrix",
    "vector",
    "tensor",
    "math"
  ],
  "license": [
    {
      "type": "MIT",
      "url": "http://g14n.info/mit-license"
    }
  ],
  "devDependencies": {
    "browserify": "^8.1.3",
    "jshint": "^2.6.3",
    "mocha": "~1.20.1",
    "should": "~4.0.4"
  },
  "engines": {
    "node": ">=0.8"
  },
  "readmeFilename": "README.md",
  "dependencies": {
    "inherits": "~2.0.1",
    "strict-mode": "~0.1.4"
  }
}
